<div>
  <% if current_user %>
    <p>Signed in as <strong><%= current_user.name %></strong>, role: <%= current_user.role %></p>
    <%= link_to "Sign out", signout_path, id: "sign_out" %>
  <% else %>
    <%= link_to "Sign in with Google", "/auth/google_oauth2", id: "sign_in" %>
  <% end %>
</div>

<h1>403</h1>
<p>Not authorized</p>
